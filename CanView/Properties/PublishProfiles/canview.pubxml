<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://canview.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>canview.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>canview</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$canview</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:cvdb.database.windows.net,1433;Initial Catalog=canview_db;User ID=canview@cvdb;Password=Welcome123" Name="Data Source=tcp:cvdb.database.windows.net,1433;Initial Catalog=canview_db;User Id=canview@cvdb;Password=Welcome123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="CanView.Models.ApplicationDbContext, CanView" MigrationConfiguration="CanView.Migrations.Configuration, CanView" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:cvdb.database.windows.net,1433;Initial Catalog=canview_db;User Id=canview@cvdb;Password=Welcome123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>